         XmNdialogTitle = 'Special Commands';
         XmNmarginWidth = 20;
!         XmNbackground = color(k_bg_colour1,background);
         XmNborderWidth = 5;
         XmNshadowThickness = 3;
      };
      controls {
         XmPushButton       button_speccmd_1;
			XmText             text_speccmd_1;
         XmPushButton       button_speccmd_2;
			XmText             text_speccmd_2;
         XmPushButton       button_speccmd_3;
			XmText             text_speccmd_3;
         XmPushButton       button_speccmd_4;
			XmText             text_speccmd_4;
         XmPushButton       button_speccmd_5;
			XmText             text_speccmd_5;
         XmPushButton       button_speccmd_6;
			XmText             text_speccmd_6;
         XmPushButton       button_speccmd_7;
			XmText             text_speccmd_7;
         XmPushButton       button_speccmd_8;
			XmText             text_speccmd_8;
         XmPushButton       button_speccmd_9;
			XmText             text_speccmd_9;
         XmPushButton       button_speccmd_10;
			XmText             text_speccmd_10;
         XmPushButton       button_speccmd_11;
			XmText             text_speccmd_11;
         XmPushButton       button_speccmd_12;
			XmText             text_speccmd_12;
         XmPushButton       button_speccmd_13;
			XmText             text_speccmd_13;
         XmPushButton       button_speccmd_14;
