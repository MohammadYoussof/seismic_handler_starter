object
   speccmd_box: XmBulletinBoardDialog {
      arguments {
         XmNdialogTitle = 'special commands';
         XmNdialogStyle = XmDIALOG_MODELESS;
!         XmNx = 600;
!         XmNy = 200;
         XmNmarginWidth = 20;
!         XmNbackground = color(k_bg_colour2,background);
         XmNbuttonFontList = k_button_font;
         XmNlabelFontList = k_label_font;
         XmNtextFontList = k_text_font;
      };
      controls {
         XmBulletinBoard speccmd_selection;
         XmRowColumn     speccmd_main_button_box;
      };
   };

object
   speccmd_main_button_box: XmRowColumn {
      arguments {
         XmNorientation = XmHORIZONTAL;
         XmNspacing = 10;
         XmNx = 110;
         XmNy = 670;
         XmNunitType = XmPIXELS;
         XmNborderWidth = 0;
         XmNentryBorder = 3;
!         XmNbackground = color(k_bg_colour2,background);
      };
      controls {
         XmPushButton     button_speccmd_hide;
      };
   };

object
   button_speccmd_hide: XmPushButton {
      arguments {
         XmNlabelString = 'Hide';
         XmNborderWidth = 5;
      };
      callbacks {
         XmNactivateCallback =
            procedure xm_call_activate( k_button_speccmd_hide );
      };
   };

object
