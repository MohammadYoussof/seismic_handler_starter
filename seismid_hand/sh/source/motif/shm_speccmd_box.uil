      };
   };

object
   button_speccmd_1: XmPushButton {
      arguments {
         XmNx = 20;
         XmNy = 20;
         XmNlabelString = '-01-';
         XmNborderWidth = 5;
      };
      callbacks {
         XmNactivateCallback =
            procedure xm_call_activate( k_button_speccmd_1 );
      };
   };

object
   text_speccmd_1: XmText {
      arguments {
         XmNx = 80;
         XmNy = 20;
         XmNwidth = 180;
         XmNunitType = XmPIXELS;
         XmNmaxLength = 60;
         XmNvalue = '1';
      };
      callbacks {
         MrmNcreateCallback =
            procedure xm_call_create_widget( k_widget_speccmd_1_text);
         XmNmodifyVerifyCallback =
            procedure xm_call_text_input( k_widget_speccmd_1_text );
      };
   };

