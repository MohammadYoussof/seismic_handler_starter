! determines & inserts signal to noise ratio of all traces of display

default 1    20.    window width (sec)
default 2    5.     noise window offset

sdef cnt 1    ! trace counter

loop_start:
   if  "cnt gti $dsptrcs  goto/forward loop_exit:
   insert_signoise "cnt #1 #2
   calc i &cnt = "cnt + 3
goto loop_start:
loop_exit:

return
